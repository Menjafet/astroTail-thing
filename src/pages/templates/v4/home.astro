import '../../stylesv4.css'
---


<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width,initial-scale=1" />
  <title>Electrician Services</title>
  <link href="https://fonts.googleapis.com/css?family=Inter" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Playfair+Display" rel="stylesheet">
  <link href="https://fonts.cdnfonts.com/css/lemonmilk" rel="stylesheet">
                
</head>
<body>
  <header>
    <nav>
      <div class="logo">
        <a href="#">Electrician Services</a>
      </div>
      <ul>
        <li><a href="#">Home</a></li>
        <li><a href="#">Services</a></li>
        <li><a href="#">Pricing</a></li>
        <li><a href="#">Testimonials</a></li>
        <li><a href="#">Contact</a></li>
      </ul>
    </nav>
  </header>

  <main>
    <section class="hero">
      <div class="hero-content">
        <h1>Welcome to Electrician Services</h1>
        <p>Reliable and professional electrical solutions for your home.</p>
        <a href="#" class="btn">Get a Quote</a>
      </div>
      <img src="https://coboelectric.ca/wp-content/uploads/2022/03/AdobeStock_284636812.jpeg" alt="Electrician Working">
    </section>

    <section class="featured-services">
      <h2>Our Services</h2>
      <div class="service">
        <img src="https://d2zhlgis9acwvp.cloudfront.net/images/uploaded/electricians.jpg" alt="Service 1">
        <h3>Service 1</h3>
        <p>Description of Service 1.</p>
      </div>
      <div class="service">
        <img src="https://www.gormanlightning.com/images/blog/Electrican.jpg" alt="Service 2">
        <h3>Service 2</h3>
        <p>Description of Service 2.</p>
      </div>
    </section>

    <section class="testimonials">
      <h2>Testimonials</h2>
      <div class="testimonial">
        <img src="https://p.turbosquid.com/ts-thumb/xz/EWf2j9/g5F8vK21/front1200x1200/jpg/1428589568/300x300/sharp_fit_q85/69c79db29f9e3063d1dfa11512b49669b30ab37a/front1200x1200.jpg" alt="Customer 1">
        <blockquote>"I'm extremely satisfied with the electrical work done in my home. Highly recommended!"</blockquote>
        <p>- Customer 1</p>
      </div>
      <div class="testimonial">
        <img src="https://p.turbosquid.com/ts-thumb/xz/EWf2j9/g5F8vK21/front1200x1200/jpg/1428589568/300x300/sharp_fit_q85/69c79db29f9e3063d1dfa11512b49669b30ab37a/front1200x1200.jpg" alt="Customer 2">
        <blockquote>"The electricians were professional, efficient, and completed the job on time. Great service!"</blockquote>
        <p>- Customer 2</p>
      </div>
    </section>

    <section class="contact">
      <h2>Contact Us</h2>
      <p>For inquiries or to schedule an appointment, please fill out the form below or call us at <strong>123-456-7890</strong>.</p>
      <form>
        <input type="text" placeholder="Name" required>
        <input type="email" placeholder="Email" required>
        <textarea placeholder="Message" required></textarea>
        <button type="submit" class="btn">Submit</button>
      </form>
    </section>
  </main>

  <footer>
    <p>&copy; 2023 Electrician Services. All rights reserved.</p>
  </footer>
</body>
</html>
